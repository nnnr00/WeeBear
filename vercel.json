{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/webhook", "dest": "api/commands.js" },
    { "src": "/start", "dest": "api/commands.js" },
    { "src": "/p", "dest": "api/commands.js" },
    { "src": "/dh", "dest": "api/commands.js" },
    { "src": "/admin", "dest": "api/commands.js" },
    { "src": "/c", "dest": "api/commands.js" },
    { "src": "/cz", "dest": "api/commands.js" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" }
      ]
    }
  ]
}
